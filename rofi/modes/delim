#!/usr/bin/env bash

if [ x"$@" = x"quit" ]
then
    exit 0
fi

# Override the previously set prompt.
# We only want to do this on first call of script.
if [ $ROFI_RETV = 0 ]
then
    echo -en "\x00delim\x1f|\n"
fi

echo -en "\x00prompt\x1fChange prompt|"
for a in {1..10}
do
    echo -en "$a|"
done
echo -en "newline\ntest|"
echo -en "quit"
